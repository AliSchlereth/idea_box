<div class="info-details">
  <h1> <%= @category.name %> </h1>
  <div class="container-fluid">
    <div class="row">
      <% @ideas.each do |idea| %>
        <div class="col-md-2">
          <%= link_to idea.name, user_idea_path(@user, idea) %>
        </div>
      <% end %>
    </div>
  </div>
</div>
